{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
      "name": "angular-athens-demo"
    },
    "apps": [
      {
        "root": "src",
        "outDir": "dist",
        "assets": [
          "assets",
          "favicon.ico"
        ],
        "index": "index.html",
        "main": "app/app.ts",
        "polyfills": "polyfills.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.app.json",
        "testTsconfig": "tsconfig.spec.json",
        "prefix": "app",
        "styles": [
          "../bower_components/angular-material/angular-material.css",
          "../bower_components/toastr/toastr.css",
          "styles.css",
          "../bower_components/components-font-awesome/css/font-awesome.css"
        ],
        "scripts": [
          "../bower_components/jquery/dist/jquery.js",
          "../bower_components/toastr/toastr.js",
          "../bower_components/angular/angular.js",
          "../bower_components/angular-animate/angular-animate.js",
          "../bower_components/angular-aria/angular-aria.js",
          "../bower_components/angular-material/angular-material.js",
          "../bower_components/angular-resource/angular-resource.js",
          "../bower_components/angular-sanitize/angular-sanitize.js",
          "../bower_components/angular-ui-router/release/angular-ui-router.js",
          "app/blocks/exception/exception.module.ts",
          "app/blocks/exception/exception-handler.provider.ts",
          "app/blocks/exception/exception.service.ts",
          "app/blocks/logger/logger.module.ts",
          "app/blocks/logger/logger.ts",
          "app/core/core.module.ts",
          "app/core/core.constants.ts",
          "app/core/core.config.ts",
          "app/core/core.routes.ts",
          "app/core/image.service.ts",
          "app/core/http-interceptor.ts",
          "app/widgets/widgets.module.ts",
          "app/widgets/character-detail.ts",
          "app/layout/layout.module.ts",
          "app/layout/shell.ts",
          "app/layout/topnav.ts",
          "app/layout/copyright.ts",
          "app/characters/characters.module.ts",
          "app/characters/characters.service.ts",
          "app/characters/character-list.ts",
          "app/characters/character-card.ts",
          "app/comics/comics.module.ts",
          "app/comics/comics.service.ts",
          "app/comics/comic-list.ts",
          "app/comics/comic-detail.ts",
          "app/quiz/quiz.module.ts",
          "app/quiz/quiz.service.ts",
          "app/quiz/quiz.ts",
          "app/quiz/question.ts",
          "app/templates.ts"
        ],
        "environmentSource": "environments/environment.ts",
        "environments": {
          "dev": "environments/environment.ts",
          "prod": "environments/environment.prod.ts"
        }
      }
    ],
    "e2e": {
      "protractor": {
        "config": "./protractor.conf.js"
      }
    },
    "lint": [
      {
        "project": "src/tsconfig.app.json",
        "exclude": "**/node_modules/**"
      },
      {
        "project": "src/tsconfig.spec.json",
        "exclude": "**/node_modules/**"
      },
      {
        "project": "e2e/tsconfig.e2e.json",
        "exclude": "**/node_modules/**"
      }
    ],
    "test": {
      "karma": {
        "config": "./karma.conf.js"
      }
    },
    "defaults": {
      "styleExt": "css",
      "component": {}
    }
  }
